    <body style>


        <!-- Barra Lateral -->
        <ul id="nav-mobile" class="side-nav fixed" style="width: 300px; transform: translateX(-100%);">
            <li class="logo">
                <div class="center-align">
                    <a id="logo-container" href="#" class="brand-logo ">
                        <img src="~/images/Logo1.png" width="125px" height="125px"/>
